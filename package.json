{
  "name": "integration-glue-pipeline",
  "version": "1.0.0",
  "description": "Integration glue pipeline for Netlify, Supabase, and Notion",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "migrate": "node scripts/migrate.sh",
    "deploy": "node scripts/deploy.sh",
    "health-check": "node src/healthCheck.js"
  },
  "keywords": [
    "integration",
    "netlify",
    "supabase",
    "notion",
    "pipeline",
    "automation"
  ],
  "author": "Ian Acosta",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
